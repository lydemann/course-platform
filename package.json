  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "test:pr": "npm run affected:test -- --base origin/main --head HEAD --parallel",
    "nx-decorate-angular-cli": "node ./decorate-angular-cli.js",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "codecov": "codecov -t ",
    "firebase-deploy": "firebase deploy",
    "deploy:course-service": "npm run firebase-deploy -- --only functions --only functions:api",
    "deploy:course-client": "npm run firebase-deploy -- --only hosting:course-client",
    "deploy:course-admin": "npm run firebase-deploy -- --only hosting:course-admin",
    "build:analog": "nx build course-platform-analog && cp apps/course-platform-analog/src/firebase.json dist/apps/course-platform-analog/analog && cp apps/course-platform-analog/src/.firebaserc dist/apps/course-platform-analog/analog",
    "deploy:analog": "npm run build:analog && cd dist/apps/course-platform-analog/analog && firebase deploy --only functions:server,hosting",
    "serve:course-service": "concurrently --kill-others \"npm run build -- course-service --source-map --watch\" \"firebase serve --only functions\"",
    "debug:course-service": "concurrently --kill-others \"npm run build -- course-service --source-map --watch\" \"firebase emulators:start --only functions --inspect-functions \"",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nx/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "graphql-codegen": "graphql-codegen --config graphql-codegen.yml",
    "prepare": "husky install",
    "generate": "cd ./apps/course-platform-analog && drizzle-kit generate",
    "migrate": "cd ./apps/course-platform-analog && drizzle-kit migrate",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ludemann-consulting-aps --project javascript-angular ./dist && sentry-cli sourcemaps upload --org ludemann-consulting-aps --project javascript-angular ./dist"
  },
