  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:pr": "npm run affected:test -- --base origin/master --head HEAD --parallel",
    "nx-decorate-angular-cli": "node ./decorate-angular-cli.js",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "codecov": "codecov -t ",
    "firebase-deploy": "firebase deploy",
    "deploy:functions": "npm run firebase-deploy -- --only functions",
    "deploy:course-client": "npm run firebase-deploy -- --only hosting:course-client",
    "deploy:course-admin": "npm run firebase-deploy -- --only hosting:course-admin",
    "course-service:serve": "concurrently --kill-others \"npm run build -- course-service --source-map --watch\" \"firebase serve --only functions\"",
    "course-service:debug": "concurrently --kill-others \"npm run build -- course-service --source-map --watch\" \"firebase emulators:start --only functions --inspect-functions \"",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ngcc": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "graphql-codegen": "graphql-codegen --config graphql-codegen.yml"
  },
