    "deploy:functions": "npm run firebase-deploy -- --only functions",
    "deploy:course-client": "npm run firebase-deploy -- --only hosting:course-client",
    "deploy:course-admin": "npm run firebase-deploy -- --only hosting:course-admin",
    "course-service:serve": "concurrently --kill-others \"npm run build -- course-service --source-map --watch\" \"firebase serve --only functions\"",
    "course-service:debug": "concurrently --kill-others \"npm run build -- course-service --source-map --watch\" \"firebase emulators:start --only functions --inspect-functions \"",
