{
  "version": 1,
  "projects": {
    "course-admin": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/course-admin",
      "sourceRoot": "apps/course-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "firebase",
              "firebase/app",
              "firebase/auth",
              "graphql-tag",
              "zen-observable"
            ],
            "outputPath": "dist/apps/course-admin",
            "index": "apps/course-admin/src/index.html",
            "main": "apps/course-admin/src/main.ts",
            "polyfills": "apps/course-admin/src/polyfills.ts",
            "tsConfig": "apps/course-admin/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/course-admin/src/favicon.ico",
              "apps/course-admin/src/assets",
              "apps/course-admin/src/.firebaserc",
              "apps/course-admin/src/firebase.json"
            ],
            "styles": ["apps/course-admin/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/ui/styles/src/lib"]
            },
            "scripts": [],
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2mb",
                "maximumError": "5mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "6kb",
                "maximumError": "10kb"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/course-admin/src/environments/environment.ts",
                  "with": "apps/course-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "course-admin:build",
            "port": 4201
          },
          "configurations": {
            "production": {
              "browserTarget": "course-admin:build:production"
            },
            "development": {
              "browserTarget": "course-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "course-admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/course-admin/**/*.ts",
              "apps/course-admin/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/course-admin/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:course-admin", "type:app"]
    },
    "course-admin-e2e": {
      "root": "apps/course-admin-e2e",
      "sourceRoot": "apps/course-admin-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/course-admin-e2e/cypress.json",
            "tsConfig": "apps/course-admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "course-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "course-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/course-admin-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": ["scope:course-admin"],
      "implicitDependencies": ["course-admin"]
    },
    "course-admin-lib": {
      "root": "libs/course-admin-lib",
