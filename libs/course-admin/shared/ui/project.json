{
  "name": "course-admin-shared-ui",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/course-admin/shared/ui/src",
  "prefix": "course-platform",
  "tags": [
    "type:ui",
    "scope:course-admin",
    "scope:course-admin-shared"
  ],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/course-admin/shared/ui/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/course-admin/shared/ui/**/*.ts",
          "libs/course-admin/shared/ui/**/*.html"
        ]
      }
    }
  }
}
